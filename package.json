    "hardhat-ethers": "^1.0.1",
    "hardhat-etherscan-abi": "^0.2.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-tracer": "*",
    "hardhat-typechain": "^0.3.5",
    "mocha": "^9.2.2",
    "solc-0.8.4": "^1.0.1",
    "solidity-coverage": "^0.7.21",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslog": "^3.3.3",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3",
    "yargs": "^17.4.1"
  },
  "engines": {
    "node": "^14.18.1"
  }
